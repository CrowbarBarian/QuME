<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="QuME_Frame">
		<title>QuME v0.1</title>
		<icon>quake2_icon.png</icon>
		<id_arg>0</id_arg>
		<object class="wxGridBagSizer" variable="GridBagSizer1" member="no">
			<object class="sizeritem">
				<object class="wxTextCtrl" name="ID_INFOCONSOLE" variable="TextCtrl1" member="yes">
					<size>400,200</size>
					<font>
						<size>10</size>
						<style>normal</style>
						<weight>normal</weight>
						<underlined>0</underlined>
						<family>teletype</family>
						<face>Liberation Mono</face>
					</font>
					<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_DONTWRAP|wxSUNKEN_BORDER|wxVSCROLL|wxHSCROLL</style>
				</object>
				<colspan>2</colspan>
				<col>0</col>
				<row>1</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="ID_STATICTEXT_FACEINSPECTOR" variable="StaticText1" member="yes">
					<label>Face Inspector:</label>
				</object>
				<col>0</col>
				<row>2</row>
				<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxSpinCtrl" name="ID_SPINCTRL1" variable="SpinCtrl1" member="yes">
					<value>0</value>
					<enabled>0</enabled>
					<tooltip>Face to Examine</tooltip>
					<handler function="OnSpinCtrl1Change" entry="EVT_SPINCTRL" />
				</object>
				<col>1</col>
				<row>2</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxSpinCtrl" name="ID_SPINCTRL2" variable="SpinCtrl2" member="yes">
					<value>0</value>
					<enabled>0</enabled>
					<handler function="OnSpinCtrl2Change" entry="EVT_SPINCTRL" />
				</object>
				<col>1</col>
				<row>3</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="ID_STATICTEXT_TEXTUREINSPECTOR" variable="StaticText3" member="yes">
					<label>Texture Inspector:</label>
				</object>
				<col>0</col>
				<row>4</row>
				<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="ID_STATICTEXT_ENTITYINSPECTOR" variable="StaticText2" member="yes">
					<label>Entity Inspector:</label>
				</object>
				<col>0</col>
				<row>3</row>
				<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxSpinCtrl" name="ID_SPINCTRL3" variable="SpinCtrl3" member="yes">
					<value>0</value>
					<enabled>0</enabled>
					<handler function="OnSpinCtrl3Change" entry="EVT_SPINCTRL" />
				</object>
				<col>1</col>
				<row>4</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="ID_STATICTEXT_INFOCONSOLE" variable="StaticText4" member="yes">
					<label>Info Console</label>
				</object>
				<col>0</col>
				<row>0</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="ID_MENU_OPEN" variable="MenuItem3" member="yes">
					<label>&amp;Open...</label>
					<accel>Alt-O</accel>
					<help>Open a BSP File</help>
				</object>
				<object class="wxMenuItem" name="ID_MENU_EXPORT" variable="MenuItem4" member="yes">
					<label>Export to .obj/.mtl</label>
					<help>Export current BSP data to Wavefront format</help>
					<enabled>0</enabled>
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="ID_MENU_QUIT" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="yes">
				<label>Options</label>
				<object class="wxMenuItem" name="ID_MENU_BASEDIR" variable="MenuItem5" member="yes">
					<label>Set Quake 2 Base Directory</label>
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="ID_MENU_ABOUT" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
			<style>wxRAISED_BORDER</style>
		</object>
	</object>
</wxsmith>
